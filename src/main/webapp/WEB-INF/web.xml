 <web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
 
 
	<display-name>Archetype Created Web Application</display-name>
	
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>test.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>register</servlet-name>
		<servlet-class>test.RegisterServlet</servlet-class>
	</servlet>


	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>register</servlet-name>
		<url-pattern>/register.do</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>showdata</servlet-name>
		<servlet-class>test.ShowDB</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>showdata</servlet-name>
		<url-pattern>/showdb</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>dbconntest</servlet-name>
		<servlet-class>test.DBConnection</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>dbconntest</servlet-name>
		<url-pattern>/dbtest</url-pattern>
	</servlet-mapping>

</web-app>

<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
	<display-name>Archetype Created Web Application</display-name>
	
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>test.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>register</servlet-name>
		<servlet-class>test.RegisterServlet</servlet-class>
	</servlet>
	


	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>register</servlet-name>
		<url-pattern>/register.do</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>bbs-list-servlet</servlet-name>
		<servlet-class>brainServlet.BBSListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>bbs-list-servlet</servlet-name>
		<url-pattern>/bbs-list</url-pattern>
	</servlet-mapping>
</web-app>

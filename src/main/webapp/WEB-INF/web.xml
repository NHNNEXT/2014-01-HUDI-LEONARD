<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
    http://java.sun.com/xml/ns/javaee
    http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>Chapter1 Servlet</servlet-name>
		<servlet-class>headFirst.Ch1Servlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Ch3 Beer</servlet-name>
		<servlet-class>headFirst.BeerSelect</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CodeReturn</servlet-name>
		<servlet-class>headFirst.CodeReturn</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>BeerParamTests</servlet-name>
		<servlet-class>headFirst.TestInitParams</servlet-class>
	</servlet>
	

	<servlet-mapping>
		<servlet-name>Chapter1 Servlet</servlet-name>
		<url-pattern>/Serv1</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Ch3 Beer</servlet-name>
		<url-pattern> /SelectBeer.do </url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CodeReturn</servlet-name>
		<url-pattern>/codeReturn</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>BeerParamTests</servlet-name>
		<url-pattern>/Tester.do</url-pattern>
	</servlet-mapping>
	
	<context-param>
		<param-name>adminEmail</param-name>
		<param-value>woonohyo@nhnnext.org</param-value>
	</context-param>
	
	<servlet>
		<servlet-name>ListenerTester</servlet-name>
		<servlet-class>headFirst.ListenerTester</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ListenerTester</servlet-name>
		<url-pattern>/ListenTest.do</url-pattern>
	</servlet-mapping>
	<context-param>
		<param-name>breed</param-name>
		<param-value> Great Dane</param-value>
	</context-param>
	<listener>
		<listener-class>headFirst.MyServletContextListener</listener-class>
	</listener>
</web-app>

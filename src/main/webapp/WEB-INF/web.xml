<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

	<display-name>Archetype Created Web Application</display-name>
	
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>test.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>register</servlet-name>
		<servlet-class>test.RegisterServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>register</servlet-name>
		<url-pattern>/register.do</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>bbs-list-servlet</servlet-name>
		<servlet-class>brainServlet.BBSListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>bbs-list-servlet</servlet-name>
		<url-pattern>/bbs-list</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>myjars-servlet</servlet-name>
		<servlet-class>test.MyJarsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>myjars-servlet</servlet-name>
		<url-pattern>myjars</url-pattern>
	</servlet-mapping>
</web-app>

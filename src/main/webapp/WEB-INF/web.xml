<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
	<display-name>Archetype Created Web Application</display-name>

	<!-- test servlet start -->
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>test.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>register</servlet-name>
		<servlet-class>test.RegisterServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login.do</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>register</servlet-name>
		<url-pattern>/register.do</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>dbconntest</servlet-name>
		<servlet-class>test.DBConnection</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>dbconntest</servlet-name>
		<url-pattern>/dbtest</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>js-servlet</servlet-name>
		<servlet-class>test.JavascriptServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>js-servlet</servlet-name>
		<url-pattern>/jstest</url-pattern>
	</servlet-mapping>
	<!-- test servlet end -->


	<!-- real development servlet start -->

	<servlet>
		<servlet-name>main-featured</servlet-name>
		<servlet-class>net.collagejam.web.MainFeaturedController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>main-featured</servlet-name>
		<url-pattern>/showdb</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>createjarservlet</servlet-name>
		<servlet-class>net.collagejam.web.CreateJamjarController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>createjarservlet</servlet-name>
		<url-pattern>/createjar</url-pattern>
	</servlet-mapping>

	<!-- real development servlet end -->

</web-app>
